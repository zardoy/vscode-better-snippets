{
    "name": "better-snippets",
    "displayName": "Better Snippets",
    "publisher": "zardoy",
    "icon": "resources/icon.png",
    "version": "0.0.0-dev",
    "web": true,
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "snippets",
        "snippets alternative",
        "insane snippets"
    ],
    "contributes": {
        "commands": [
            {
                "command": "completionInsert",
                "title": "CompletionInsert"
            },
            {
                "command": "applySpecialSnippet",
                "title": "applySpecialSnippet"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "completionInsert",
                    "when": "false"
                }
            ]
        }
    },
    "activationEvents": [
        "*"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": true
    },
    "repository": "https://github.com/zardoy/vscode-fix-all-json.git",
    "author": "Vitaly Turovsky <vital2580@icloud.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "vscode-framework start",
        "start-web": "vscode-framework start --web",
        "build": "vscode-framework build",
        "test": "echo not-implemented"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.17",
        "@types/vscode": "^1.63.1",
        "@vscode/test-electron": "^2.0.1",
        "@zardoy/tsconfig": "^1.2.2",
        "char": "^1.0.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.6.0",
        "eslint-config-zardoy": "^0.2.5",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@zardoy/utils": "^0.0.4",
        "got": "^11.8.3",
        "rambda": "^6.9.0",
        "ts-essentials": "^9.1.2",
        "type-fest": "^2.8.0",
        "vscode-framework": "^0.0.18",
        "vscode-snippet-parser": "^0.0.5"
    }
}
